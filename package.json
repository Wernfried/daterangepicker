{
  "name": "daterangepicker-4.x",
  "version": "4.0.0-0",
  "description": "Date range picker with time component",
  "main": "daterangepicker.js",
  "style": "daterangepicker.css",
  "jsdelivr": [
    "daterangepicker.js",
    "daterangepicker.css"
  ],
  "scripts": {
    "preversion": "jsdoc2md --private daterangepicker.js > daterangepicker.md",
